: 1758504192:0;\
\
#\
LOGFILE="/tmp/waybar_cpu_memory.log"\
echo "---- $(date) ----" >>"$LOGFILE"\
\
#\
CPU_USAGE=$(top -bn1 | grep "Cpu(s)" | awk '{print $2 + $4}' | awk '{printf "%d", $1}')\
if [ -z "$CPU_USAGE" ] || ! [[ "$CPU_USAGE" =~ ^[0-9]+$ ]]; then\
 CPU_USAGE=0\
 echo "Warning: CPU_USAGE invalid or empty, defaulting to 0" >>"$LOGFILE"\
fi\
\
#\
MEMORY=$(free -m | awk '/Mem:/ {printf "%.1f %.1f", $3/1024, $2/1024}')\
USED=$(echo "$MEMORY" | awk '{print $1}')\
TOTAL=$(echo "$MEMORY" | awk '{print $2}')\
if [ -z "$USED" ] || [ -z "$TOTAL" ] || [ "$TOTAL" = "0.0" ]; then\
 MEMORY_PERCENT=0\
 echo "Warning: Memory values invalid or empty, defaulting to 0%" >>"$LOGFILE"\
else\
 MEMORY_PERCENT=$(echo "$USED $TOTAL" | awk '{printf "%d", ($1 / $2) * 100}')\
 echo "Memory: USED=$USED, TOTAL=$TOTAL, PERCENT=$MEMORY_PERCENT" >>"$LOGFILE"\
fi\
\
#\
JSON="{\"text\": \"$CPU_USAGE% ó°»ƒ€ ${MEMORY_PERCENT}% ó°ƒ­ƒ»\", \"tooltip\": \"CPU: $CPU_USAGE% | Memory: ${MEMORY_PERCENT}%\", \"class\": \"cpu-memory\"}"\
echo "$JSON" >>"$LOGFILE"\
echo "$JSON"\
\
#\
if [ -n "$WAYBAR_CLICKED" ]; then\
 alacritty -e btop &\
 exit 0\
fi
